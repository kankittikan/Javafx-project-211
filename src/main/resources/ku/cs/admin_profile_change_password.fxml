<AnchorPane fx:id="mainAnchorPane" prefHeight="720" prefWidth="1024" styleClass="AnchorPane4" stylesheets="@css/backup/AnchorPane.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.report.controllers.AdminProfileChangePasswordController">
    <children>
        <AnchorPane fx:id="changePasswordAnchorPane" layoutX="187.0" layoutY="85.0" prefHeight="550.0" prefWidth="650.0" style="-fx-background-radius: 15;">
            <effect>
                <DropShadow height="124.5" radius="62.125" spread="0.22" width="126.0">
                    <color>
                        <Color red="0.8313725590705872" green="0.8549019694328308" blue="0.8627451062202454" />
                    </color>
                </DropShadow>
            </effect>
            <children>
                <Label fx:id="newPasswordStatus" layoutX="469.0" layoutY="304.0" text="newPasswordStatus">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="oldPasswordStatus" layoutX="469.0" layoutY="224.0" text="oldpasswordStatus">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="usernameStatus" layoutX="469.0" layoutY="144.0" text="usernameStatus">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="usernameField" layoutX="220.0" layoutY="138.0" prefHeight="31.0" prefWidth="210.0" promptText="Username" />
                <PasswordField fx:id="oldPasswordField" layoutX="220.0" layoutY="218.0" prefHeight="31.0" prefWidth="210.0" promptText="Password" />
                <PasswordField fx:id="newPasswordField" layoutX="220.0" layoutY="298.0" prefHeight="31.0" prefWidth="210.0" promptText="New Password" />
                <PasswordField fx:id="confirmNewPasswordFiled" layoutX="220.0" layoutY="380.0" prefHeight="31.0" prefWidth="210.0" promptText="Confirm New Password" />
                <Button fx:id="enterButton" layoutX="430.0" layoutY="456.0" mnemonicParsing="false" onAction="#handleChangePasswordButton" prefHeight="30.0" prefWidth="70.0" style="-fx-background-radius: 5;" text="ยืนยัน">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
                <Label fx:id="status" alignment="CENTER" layoutX="193.0" layoutY="76.0" prefHeight="27.0" prefWidth="285.0" text="status">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="changePasswordLabel" layoutX="266.0" layoutY="23.0" text="เปลี่ยนรหัสผ่าน">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="usernameHeadLabel" layoutX="59.0" layoutY="144.0" text="ชื่อผู้ใช้">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="passwordHeadLabel" layoutX="62.0" layoutY="224.0" text="รหัสผ่าน">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="confirmNewPasswordLabel" layoutX="60.0" layoutY="386.0" text="ยืนยันรหัสผ่านใหม่">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="newPasswordHeadLabel" layoutX="59.0" layoutY="304.0" text="รหัสผ่านใหม่">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="navAnchorPane" prefHeight="70.0" prefWidth="1024.0">
            <children>
                <Label fx:id="time" layoutX="73.0" layoutY="25.0" text="Time">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="usernameLabel" layoutX="848.0" layoutY="20.0" text="username">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <ImageView fx:id="image" fitHeight="50.0" fitWidth="65.0" layoutX="757.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" />
            </children>
        </AnchorPane>
        <Button fx:id="backButton" layoutX="14.0" layoutY="642.0" mnemonicParsing="false" onAction="#handleBackToProfileButton" prefHeight="64.0" prefWidth="118.0" text="ย้อนกลับ">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
