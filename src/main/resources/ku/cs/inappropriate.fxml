<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="720.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.report.controllers.InappropriateController">
    <children>
        <BorderPane prefHeight="720.0" prefWidth="1024.0">
            <top>
                <AnchorPane fx:id="mainAnchorPane" prefHeight="82.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="dateTime" layoutX="71.0" layoutY="28.0" text="Time">
                            <font>
                                <Font name="System Bold" size="14.0" />
                            </font>
                        </Label>
                        <ImageView fx:id="miniImageStudent" fitHeight="25.0" fitWidth="25.0" layoutX="810.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true" />
                    </children>
                </AnchorPane>
            </top>
         <bottom>
                <AnchorPane fx:id="bodyAnchorPane" prefHeight="643.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="headReportLabel" layoutX="764.0" layoutY="80.0" text="HeadLabel" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="categoryReportLabel" layoutX="752.0" layoutY="37.0" text="CategoryLabel" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="timeReportLabel" layoutX="769.0" layoutY="169.0" text="TimeLabel" textAlignment="CENTER">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="categoryLabel" layoutX="628.0" layoutY="37.0" text="หมวดหมู่">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="headLabel" layoutX="650.0" layoutY="80.0" text="หัวข้อ">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="timeLabel" layoutX="630.0" layoutY="169.0" text="เวลาแจ้ง">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Label>
                        <TextArea fx:id="subjectTextArea" editable="false" layoutX="591.0" layoutY="203.0" prefHeight="182.0" prefWidth="370.0" wrapText="true">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </TextArea>
                        <Button fx:id="backButton" layoutX="39.0" layoutY="566.0" mnemonicParsing="false" onAction="#handleBackButton" text="กลับ">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Button>
                        <Label fx:id="myReportLabel" layoutX="66.0" layoutY="37.0" text="เรื่องร้องเรียน">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                        </Label>
                        <Button fx:id="Delete" layoutX="850.0" layoutY="403.0" mnemonicParsing="false" onAction="#handleDeleteReport" text="ลบรายการ">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Button>
                        <Button fx:id="Ban" layoutX="838.0" layoutY="464.0" mnemonicParsing="false" onAction="#handleBanUserButton" text="ระงับสิทธิ์ผู้ใช้">
                            <font>
                                <Font size="19.0" />
                            </font>
                        </Button>
                        <Button fx:id="Petition" layoutX="829.0" layoutY="524.0" mnemonicParsing="false" onAction="#handleRequestUnbanButton" text="คำร้องขอคืนสิทธิ์">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Button>
                        <ListView fx:id="listViewReport" layoutX="47.0" layoutY="94.0" prefHeight="437.0" prefWidth="531.0" />
                  <Label fx:id="noticeLabel" layoutX="245.0" layoutY="37.0" text="noticeLabel">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="categoryLabel1" layoutX="622.0" layoutY="126.0" text="ผู้ถูกร้องเรียน">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="nameLabel" layoutX="763.0" layoutY="126.0" text="nameLabel" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <ImageView fx:id="imageView" fitHeight="121.0" fitWidth="144.0" layoutX="615.0" layoutY="404.0" pickOnBounds="true" preserveRatio="true" />
